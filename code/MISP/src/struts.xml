<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">


<struts>
<constant name="struts.ui.theme" value="simple" />

<constant name="struts.ui.templateDir" value="template" />

<constant name="struts.ui.templateSuffix" value="ftl" />
	<package name="login" extends="struts-default">
		<action name="login" class="cn.fuego.misp.web.action.login.LoginAction">
			<result type="dispatcher">/client/platform/main/main.jsp</result>
			<result type="dispatcher" name="LoginFailed">/client/platform/login/login.jsp</result>		    
		</action>
		
		<action name="userManage" class="cn.fuego.misp.web.action.user.UserManageAction">
			<result type="dispatcher">/client/platform/userInfo/userManage.jsp</result>  
		</action>
		
		<action name="ajaxDownloadDebug" class="cn.fuego.misp.web.action.debug.AjaxDownDebugAction">
			<result type="dispatcher">/client/platform/debug/ajaxdown.jsp</result> 
		</action>

 	</package>
</struts>

