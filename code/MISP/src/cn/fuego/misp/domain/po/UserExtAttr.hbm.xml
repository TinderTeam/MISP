<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.misp.domain.po">
	<class name="UserExtAttr" table="D_USER_EXT_ATTR">
		<composite-id>
	       
           <key-property name="userID" column="USER_ID" ></key-property>
           <key-property name="attrName" column="ATTR_NAME"  ></key-property>
           
	    </composite-id>
        
           <property name="attrValue" column="ATTR_VALUE"  not-null="false" ></property>

				
	</class>
</hibernate-mapping>