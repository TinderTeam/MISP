<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.misp.domain.po">
	<class name="Org" table="D_ORG" lazy="false">
			<id name="orgID" column="ORG_ID"  type="int">
			<generator class="assigned"/>
			</id>
           <property name="orgName" column="ORG_NAME"  not-null="true" ></property>
           <property name="orgDesp" column="ORG_DESP"  not-null="false" ></property>
           <property name="fatherID" column="ORG_FATHERID"  not-null="false" ></property>
	</class>
</hibernate-mapping>