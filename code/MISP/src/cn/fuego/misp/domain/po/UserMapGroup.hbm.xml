<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.misp.domain.po">
	<class name="UserMapGroup" table="D_USER_MAP_GROUP">
		<composite-id>
	       
           <key-property name="userID" column="USER_ID"  type="java.lang.String"></key-property>
           <key-property name="groupID" column="GROUP_ID"  type="java.lang.String"></key-property>
           
	    </composite-id>


				
	</class>
</hibernate-mapping>