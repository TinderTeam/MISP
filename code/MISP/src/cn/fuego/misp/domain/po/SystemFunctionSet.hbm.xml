<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.misp.domain.po">
	<class name="SystemFunctionSet" table="S_SYSTEM_FUNCTION_SET">
			<id name="functionID" column="FUNCTION_ID"  type="java.lang.String">
			<generator class="assigned"/>
			</id>
        
           <property name="functionName" column="FUNCTION_NAME"  not-null="true" ></property>
           <property name="functionType" column="FUNCTION_TYPE"  not-null="true" ></property>
           <property name="functionDesp" column="FUNCTION_DESP"  not-null="false" ></property>	
				
	</class>
</hibernate-mapping>